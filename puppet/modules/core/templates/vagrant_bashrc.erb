# .bashrc
# Source global definitions
if [ -f /etc/bashrc ]; then
	. /etc/bashrc
fi

# User specific aliases and functions

alias cdu='cd ..'
alias reloadbash='. ~/.bashrc;'

function grunt_run {
	npm install -g grunt-cli
	npm install
	grunt
}

function() goto {
	# $1 alias to path
	

	if [ "$1" == "js" ]; then
		cd <%= @base_path %>/libraries
	fi
}

function() init_curator {
	cd <%= @base_path %>/cms/config/
	chmod +x init_curator.sh
	./init_curator.sh 
}